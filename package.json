{
  "name": "typescript-react-template",
  "version": "0.4.0",
  "description": "a typescript react template",
  "scripts": {
    "start": "yarn dev",
    "dev": "webpack serve --config scripts/webpack.dev.config.ts --hot --color",
    "build": "webpack --config scripts/webpack.prod.config.ts --color",
    "build:ssr": "export SSR=1 && yarn build && ts-node scripts/prerender.ts",
    "build:static": "export SSR_STATIC=1 && yarn build:ssr",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit --project .",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [],
  "author": "lawvs",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/html-webpack-template": "^6.0.5",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.23",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-merge": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.12.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.0",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  }
}
