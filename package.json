{
  "name": "typescript-react-template",
  "version": "0.4.0",
  "description": "a typescript react template",
  "keywords": [],
  "license": "MIT",
  "author": "lawvs",
  "scripts": {
    "build": "webpack --config scripts/webpack.prod.config.ts --color",
    "build:ssr": "export SSR=1 && pnpm build && ts-node scripts/prerender.ts",
    "build:static": "export SSR_STATIC=1 && pnpm build:ssr",
    "clean": "rm -rf dist coverage",
    "dev": "webpack serve --config scripts/webpack.dev.config.ts --hot --color",
    "lint": "eslint .  --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm run dev",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "typecheck": "tsc --noEmit --project ."
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/html-webpack-template": "^6.0.9",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.11.0"
}
